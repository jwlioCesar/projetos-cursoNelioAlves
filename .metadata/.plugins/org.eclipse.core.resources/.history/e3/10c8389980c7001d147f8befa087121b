package entities;

public class Alura {

	private static String name;

	public Alura(String name) {
		super();
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	public static String gerName() {
		return name + "auau";
	}
	
}
